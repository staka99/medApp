pragma solidity ^0.8.0;

contract AccessControl {

    mapping(address => mapping(address => bool)) public access;

    function grantAccess(address _doctor) public {
        access[msg.sender][_doctor] = true;
    }

    function revokeAccess(address _doctor) public {
        access[msg.sender][_doctor] = false;
    }

    function isAccessGranted(address _doctor) public view returns (bool) {
        return access[msg.sender][_doctor];
    }
}
